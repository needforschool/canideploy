    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/throttler": "^4.0.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "fastify": "^4.13.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",

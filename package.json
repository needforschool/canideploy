    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.3",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.0.0",
